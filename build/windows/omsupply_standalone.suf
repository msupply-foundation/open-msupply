<Events>
<Event>
<Name>On Preload</Name>
<Args/>
<Script>-- These actions are performed before the screen is shown.
local sBodyWelcomeText = SessionVar.Expand("Welcome to the installer for %ProductName% %ProductVer%.\r\n\r\n");

if hasOmsupplyInstalled then
	sBodyWelcomeText = SessionVar.Expand("Welcome to the upgrader for %ProductName% %ProductVer%.\r\n\r\n");
end

local sBodyInfoText = sBodyWelcomeText .. "It is strongly recommended that you exit all Windows programs before continuing with this installation.\r\n\r\n";
sBodyInfoText = sBodyInfoText .. "If you have any other programs running, please click Cancel, close the programs, and run this setup again.\r\n\r\n";
sBodyInfoText = sBodyInfoText .. "Otherwise, click Next to continue.";

DlgStaticText.SetProperties(CTRL_STATICTEXT_BODY, {Text = sBodyInfoText});
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
<Name>On Back</Name>
<Args/>
<Script>-- These actions are performed when the Back button is clicked.

-- go back to the previous screen
Screen.Back();
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
<Name>On Next</Name>
<Args/>
<Script>-- These actions are performed when the Next button is clicked.
local sNextScreenName = "License Agreement";

if (hasOmsupplyInstalled) then
	sNextScreenName = "Ready to Install";
end

-- advance to the next screen
Screen.Jump(sNextScreenName);
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
