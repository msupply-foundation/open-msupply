<Event>
<Name>On Post Install</Name>
<Args/>
<Script>local omSupplyServerPath = SessionVar.Expand("%AppFolder%\\server\\omSupply-sqlite.exe");
local omSupplyDesktopPath = SessionVar.Expand("%AppFolder%\\open mSupply.exe");
local omSupplyServerDatabase = "sqlite";


if hasOmsupplyStandaloneInstalled then
	local uninstallPath = "SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Uninstall";
	local installedApps = Registry.GetKeyNames(HKEY_LOCAL_MACHINE, uninstallPath);
	if (installedApps ~= nil) then	
		-- Remove previous versions of this app from the uninstall list
		if (Table.Count(installedApps) &gt; 0) then
			for index, value in pairs(installedApps) do
				if ((String.Find(value, "omSupply Standalone", 1, false) ~= -1) and (value ~= SessionVar.Expand("omSupply Standalone%ProductVer%"))) then
					Registry.DeleteKey(HKEY_LOCAL_MACHINE, uninstallPath .. "\\" .. value);
				end
			end
		end
	end
else
	local config = "server:\r\n"
	config = config .. "  host: 127.0.0.1\r\n";
	config = config .. "  port: " .. SessionVar.Get("%omsupply_port%") .. "\r\n";
	config = config .. "database:\r\n";
	config = config .. "  database_name: \"" .. SessionVar.Get("%omsupply_db_name%") .. "\"\r\n";
	config = config .. "logging:\r\n";
	config = config .. "  mode: File\r\n";
	config = config .. "  level: Warn\r\n";
	config = config .. "  directory: log\r\n";
	config = config .. "  filename: omsupply.log\r\n";
	config = config .. "  max_file_count: 10\r\n";
	config = config .. "  max_file_size: 1\r\n";
	TextFile.WriteFromString(SessionVar.Expand("%AppFolder%\\server\\configuration\\local.yaml"), config, false);

	-- Create omSupply Server keys in registry
	Registry.CreateKey(HKEY_LOCAL_MACHINE, "Software\\omSupply Standalone");
	Registry.SetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Standalone", "Path", omSupplyServerPath, REG_SZ);
	Registry.SetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Standalone", "Database", "sqlite", REG_SZ);
end 

-- Add a firewall rule to allow incoming requests
local firewallRuleKey = "SYSTEM\\CurrentControlSet\\Services\\SharedAccess\\Parameters\\FirewallPolicy\\FirewallRules";
if (Registry.DoesKeyExist(HKEY_LOCAL_MACHINE, firewallRuleKey)) then

	Registry.SetValue(HKEY_LOCAL_MACHINE, firewallRuleKey, "open-mSupply-Server-TCP-IN", "v2.28|Action=Allow|Active=TRUE|Dir=In|Protocol=6|LPort=" .. SessionVar.Get("%omsupply_port%") .. "|Name=".. SessionVar.Get("%ProductName%") .. "|", REG_SZ);
	
	-- The firewall service requires restarting to detect the new rule
	local firewallServiceName = "Windows Firewall";
	queryResult = Service.Query(firewallServiceName, "");
	if (queryResult == SERVICE_RUNNING) then
		Service.Stop(firewallServiceName, "", 5);
		Service.Start(firewallServiceName, "", nil);
	end
end

Service.Create(SessionVar.Expand("%AppFolder%\\server\\omSupply-sqlite.exe"), "Open mSupply Server", "omsupply-server", SERVICE_WIN32_OWN_PROCESS, false, SERVICE_AUTO_START, SERVICE_ERROR_NORMAL, "", nil, nil, "", "");

Service.Start("Open mSupply Server", "omsupply-server", nil);

if not hasOmsupplyStandaloneInstalled then
	-- Create omSupply Desktop keys in registry
	Registry.CreateKey(HKEY_LOCAL_MACHINE, "Software\\omSupply Standalone");
	Registry.SetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Standalone", "Path", omSupplyDesktopPath, REG_SZ);

	Shell.CreateShortcut(Shell.GetFolder(SHF_DESKTOP), "omSupply Desktop", omSupplyDesktopPath, "", "", "", 0, SW_SHOWNORMAL, nil, "", START_OPTION_DEFAULT);
end
</Script>
