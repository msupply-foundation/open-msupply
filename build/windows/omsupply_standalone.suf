<Event>
<Name>On Startup</Name>
<Args/>
<Script>-- Set window title
local sWindowTitle = "Open mSupply Setup";
local omSupplyStandalonePath = "";

hasOmsupplyDesktopInstalled = false;
omSupplyDesktopPath = SessionVar.Expand("%AppFolder%\\open mSupply.exe");

-- Check if the omSupply Server has been already installed on this machine
if Registry.DoesKeyExist(HKEY_LOCAL_MACHINE, "Software\\omSupply Server") then
	local omSupplyServerPath = Registry.GetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Server", "Path", true);
	if File.DoesExist(omSupplyServerPath) then
		Dialog.Message("Error!", "This computer has open mSupply server installed. Installation cannot continue.", MB_OK, MB_ICONSTOP);
		Application.Exit();
	end
end

-- Check if the omSupply Desktop has been already installed on this machine
if Registry.DoesKeyExist(HKEY_LOCAL_MACHINE, "Software\\omSupply Desktop") then
	local omSupplyDesktopPath = Registry.GetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Desktop", "Path", true);
	if File.DoesExist(omSupplyDesktopPath) then
		Dialog.Message("Error!", "This computer has open mSupply desktop installed. Installation cannot continue.", MB_OK, MB_ICONSTOP);
		Application.Exit();
	end
end

-- Check if the omSupply Standalone has been already installed on this machine
if Registry.DoesKeyExist(HKEY_LOCAL_MACHINE, "Software\\omSupply Standalone") then
	omSupplyStandalonePath = Registry.GetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Standalone", "Path", true);
	if File.DoesExist(omSupplyStandalonePath) then
		hasOmsupplyStandaloneInstalled = true;
	end
end

hasOmsupplyStandaloneInstalled = File.DoesExist(omSupplyStandalonePath);
if hasOmsupplyStandaloneInstalled then
	sWindowTitle = "Open mSupply Upgrader";
end

SessionVar.Set("%window_title%", sWindowTitle);</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
<Name>On Pre Install</Name>
<Args/>
<Script>-- Check if the open msupply service already exist or is running at the moment.
-- Stop (if running) the service and then delete it for a fresh new service installation.
local bServiceExists = false;
queryResult = Service.Query("Open mSupply Server", "omsupply-server");
if (queryResult == SERVICE_RUNNING) then
	bServiceExists = true;
	Service.Stop("Open mSupply Server", "omsupply-server", 5);
end

if ((queryResult == SERVICE_PAUSED) or (queryResult == SERVICE_STOPPED)) then
	bServiceExists = true;
end

if (bServiceExists) then
	Service.Delete("Open mSupply Server", "omsupply-server");
end
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
