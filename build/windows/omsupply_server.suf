<Event>
<Name>On Post Install</Name>
<Args/>
<Script>local sFileToBeDeletePath = "";
local omSupplyServerPath = SessionVar.Expand("%AppFolder%\\omSupply-sqlite.exe");
local omSupplyServerDatabase = SessionVar.Get("%omsupply_selected_db%");

-- Get current database name from registry (if already installed)
if hasOmsupplyInstalled then
	omSupplyServerDatabase = Registry.GetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Server", "Database", true);
end

if (omSupplyServerDatabase == "postgres") then
	omSupplyServerPath = SessionVar.Expand("%AppFolder%\\omSupply-postgres.exe"); 
end

if not hasOmsupplyInstalled then
	local config = "server:\r\n"
	config = config .. "  host: 192.168.178.68\r\n";
	config = config .. "  port: " .. SessionVar.Get("%omsupply_port%") .. "\r\n";
	config = config .. "database:\r\n";
	config = config .. "  port: " .. SessionVar.Get("%postgres_db_port%") .. "\r\n";
	config = config .. "  username: \"" .. SessionVar.Get("%postgres_db_user%") .. "\"\r\n";
	config = config .. "  password: \"" .. SessionVar.Expand("%postgres_db_pass%") .. "\"\r\n";
	config = config .. "  database_name: \"" .. SessionVar.Get("%omsupply_db_name%") .. "\"\r\n";
	config = config .. "logging:\r\n";
	config = config .. "  mode: File\r\n";
	config = config .. "  level: Warn\r\n";
	config = config .. "  directory: log\r\n";
	config = config .. "  filename: omsupply.log\r\n";
	config = config .. "  max_file_count: 5\r\n";
	config = config .. "  max_file_size: 10\r\n";
	TextFile.WriteFromString(SessionVar.Expand("%AppFolder%\\configuration\\local.yaml"), config, false);

	-- Create omSupply Server keys in registry
	Registry.CreateKey(HKEY_LOCAL_MACHINE, "Software\\omSupply Server");
	Registry.SetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Server", "Path", omSupplyServerPath, REG_SZ);
	Registry.SetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Server", "Database", omSupplyServerDatabase, REG_SZ);
end

if (omSupplyServerDatabase == "sqlite") then
	Service.Create(SessionVar.Expand("%AppFolder%\\omSupply-sqlite.exe"), "Open mSupply Server", "omsupply-server", SERVICE_WIN32_OWN_PROCESS, false, SERVICE_AUTO_START, SERVICE_ERROR_NORMAL, "", nil, nil, "", "");
	sFileToBeDeletePath = SessionVar.Expand("%AppFolder%\\omSupply-postgres.exe");
else
	Service.Create(SessionVar.Expand("%AppFolder%\\omSupply-postgres.exe"), "Open mSupply Server", "omsupply-server", SERVICE_WIN32_OWN_PROCESS, false, SERVICE_AUTO_START, SERVICE_ERROR_NORMAL, "", nil, nil, "", "");
	sFileToBeDeletePath = SessionVar.Expand("%AppFolder%\\omSupply-sqlite.exe");
end

File.Delete(sFileToBeDeletePath); -- Delete unchoosen app file (.exe) by a user

Service.Start("Open mSupply Server", "omsupply-server", nil);
</Script>
