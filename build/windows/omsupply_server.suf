<Event>
<Name>On Next</Name>
<Args/>
<Script>-- These actions are performed when the Next button is clicked.

-- Check the required fields values before sending to the next screen
local bAllOK = true;
if SessionVar.Get("%postgres_db_user%") == "" then
	Dialog.Message("Error", "User field cannot be empty.",  MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	bAllOK = false;
end

if SessionVar.Get("%postgres_db_pass%") == "" then
	btnConfirm = Dialog.Message("Warning", "You have not entered the password. Clicking OK will save the password field as blank for the PostgresSQL server.",  MB_OKCANCEL, MB_ICONQUESTION, MB_DEFBUTTON2);
	if (btnConfirm == IDCANCEL) then
		bAllOK = false;
	end
end

if ((SessionVar.Get("%postgres_db_port%") == "") or (SessionVar.Get("%postgres_db_port%") == 0)) then
	Dialog.Message("Error", "Port field cannot be empty or 0.",  MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	bAllOK = false;
end

if bAllOK then
	-- advance to the next screen
	Screen.Next();
end
</Script>
