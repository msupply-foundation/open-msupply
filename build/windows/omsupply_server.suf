<Event>
<Name>On Startup</Name>
<Args/>
<Script>-- Set window title
local sWindowTitle = "omSupply Server Setup";
local omSupplyServerPath = "";

-- Check if the omSupply Server has been already installed on this machine
if Registry.DoesKeyExist(HKEY_LOCAL_MACHINE, "Software\\omSupply Server") then
	omSupplyServerPath = Registry.GetValue(HKEY_LOCAL_MACHINE, "Software\\omSupply Server", "Path", true);
end

hasOmsupplyInstalled = File.DoesExist(omSupplyServerPath);
if hasOmsupplyInstalled then
	sWindowTitle = "omSupply Server Upgrader Setup";
end

SessionVar.Set("%window_title%", sWindowTitle);
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
<Name>On Pre Install</Name>
<Args/>
<Script>-- Callback function for HTTP.DownloadSecure
function DownloadCallback (nDownloaded, nTotal, TransferRate, SecondLeft, SecondsLeftFormat, Message)
    -- Convert total and downloaded bytes into formatted strings
    sDownloaded = String.GetFormattedSize(nDownloaded, FMTSIZE_AUTOMATIC, true);
    sTotal = String.GetFormattedSize(nTotal, FMTSIZE_AUTOMATIC, true);

    StatusDlg.SetTitle("Downloading postgreSQL. . . ");
    StatusDlg.SetMessage("Currently downloading postgreSQL installer . . . Time Left: " .. SecondsLeftFormat);
    StatusDlg.SetStatusText("Downloaded: " .. sDownloaded .. " / " .. sTotal);
    -- Set meter range (max range = 65534)
    StatusDlg.SetMeterRange(0, 65534);
    -- Set meter position (fraction downloaded * max meter range)
    StatusDlg.SetMeterPos((nDownloaded / nTotal) * 65534);
end

if (cbPostgres) then
  -- Check to see if the user is connected to the internet
  connected = HTTP.TestConnection("http://msupply.foundation", 20, 80, nil, nil);

  if connected then
    StatusDlg.Show(0, false);
    HTTP.DownloadSecure("https://get.enterprisedb.com/postgresql/postgresql-14.4-1-windows-x64.exe", _TempFolder .. "\\postgresql-14.4-1-windows-x64.exe", MODE_BINARY, 20, 443, nil, nil, DownloadCallback);

    if (Application.GetLastError() ~= 0) then
      local errorInfo = HTTP.GetHTTPErrorInfo();
      local errorText = errorInfo.Message .. "\r\nServer Status: " .. errorInfo.Status;
      Dialog.Message("Error Downloading PostgreSQL installer: " .. errorInfo.Number, errorText, MB_OK, MB_ICONEXCLAMATION);
    else
  
      StatusDlg.Hide();
      StatusDlg.Show(MB_ICONINFORMATION, false);
      StatusDlg.SetMessage("Hold on, installing postgreSQL")
      StatusDlg.SetStatusText("Installing...");
      StatusDlg.SetMeterRange(0, 1);
      local args = SessionVar.Expand("--disable-components pgAdmin,stackbuilder --enable-components server,commandlinetools --superaccount %postgres_db_user% --superpassword %postgres_db_pass% --mode unattended --serverport %postgres_db_port%")
      result = File.Run(_TempFolder .. "\\postgresql-14.4-1-windows-x64.exe", args, "", SW_SHOWNORMAL, true);
      StatusDlg.SetMeterPos(1);
      StatusDlg.Hide();
      
      if (result ~= 0) then
        Dialog.Message("PostgreSQL Install Failed", "Something went wrong with the postgreSQL installation.", MB_OK, MB_ICONEXCLAMATION)
      end
    end
  else
    Dialog.Message("Internet Error", "You are not connected to the internet. Please connect to download.");
  end
end

-- Check if the open msupply service already exist or is running at the moment.
-- Stop (if running) the service and then delete it for a fresh new service installation.
local bServiceExists = false;
queryResult = Service.Query("Open mSupply Server", "omsupply-server");
if (queryResult == SERVICE_RUNNING) then
	bServiceExists = true;
	Service.Stop("Open mSupply Server", "omsupply-server", 5);
end

if ((queryResult == SERVICE_PAUSED) or (queryResult == SERVICE_STOPPED)) then
	bServiceExists = true;
end

if (bServiceExists) then
	Service.Delete("Open mSupply Server", "omsupply-server");
end
</Script>
