<Events>
<Event>
<Name>On Preload</Name>
<Args/>
<Script>-- These actions are performed before the screen is shown.
if (SessionVar.Get("%omsupply_selected_db%") == "sqlite") then
	DlgRadioButton.SetProperties(CTRL_RADIO_BUTTON_01, {Checked=true});
	DlgRadioButton.SetProperties(CTRL_RADIO_BUTTON_02, {Checked=false});
else
	DlgRadioButton.SetProperties(CTRL_RADIO_BUTTON_01, {Checked=false});
	DlgRadioButton.SetProperties(CTRL_RADIO_BUTTON_02, {Checked=true});
	SessionVar.Set("%omsupply_selected_db%", "postgres");
end
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
<Name>On Back</Name>
<Args/>
<Script>-- These actions are performed when the Back button is clicked.
tRbSqliteProps = DlgRadioButton.GetProperties(CTRL_RADIO_BUTTON_01);
if not tRbSqliteProps.Checked then
	SessionVar.Set("%omsupply_selected_db%", "postgres");
else
	SessionVar.Set("%omsupply_selected_db%", "sqlite");
end
-- go back to the previous screen
Screen.Back();
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
<Name>On Next</Name>
<Args/>
<Script>-- These actions are performed when the Next button is clicked.
local sNextScreenName = "Select Install Folder";

tRbSqliteProps = DlgRadioButton.GetProperties(CTRL_RADIO_BUTTON_02); -- If postgres is selected as db
if tRbSqliteProps.Checked then
	SessionVar.Set("%omsupply_selected_db%", "postgres");
	sNextScreenName = "Postgres Configuration";
else
	SessionVar.Set("%omsupply_selected_db%", "sqlite");
end

-- advance to the next screen
Screen.Jump(sNextScreenName);
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
<Name>On Cancel</Name>
<Args/>
<Script>-- These actions are performed when the Cancel button is clicked.

-- from _SUF70_Global_Functions.lua:
-- ask user if they're sure they want to exit
if g_ConfirmSetupAbort() then
	Application.Exit(EXIT_REASON_USER_ABORTED);
end
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
